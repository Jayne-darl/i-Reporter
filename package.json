{
  "name": "i-reporter",
  "version": "1.0.0",
  "description": "Api Endpoints for iReporter",
  "main": "app.js",
  "scripts": {
    "start": "babel-node server/index.js",
    "devstart": "nodemon app.js --exec babel-node",
